%h2 Iron Yard Courses

%ul 
  - @courses.each do |courses|
    %li
    = courses.name
    %br
    = link_to "View", course_path(courses), class: 'btn btn-info'
    = link_to "Edit", edit_course_path(courses), class: 'btn btn-warning'
    = link_to "Delete", courses_path(courses), method: :delete, class: 'btn btn-danger'

= link_to "Create New Course", new_course_path, class: 'btn btn-success'

= link_to "Home", root_path, class: 'btn btn-success'